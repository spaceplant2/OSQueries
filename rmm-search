SELECT * FROM
(
  SELECT
    name AS 'Program Name',
    version AS Version,
    install_date AS 'Install Date',
    install_location AS 'Install Location',
    install_source AS 'Install Source',
    '' AS 'Auto Launch',
    publisher AS Publisher,
    uninstall_string AS 'Uninstall String'
  FROM programs
  UNION ALL
  SELECT
    '' AS 'Program Name',
    '' AS Version,
    '' AS 'Install Date',
    '' AS 'Install Location',
    '' AS 'Install Source',
    name AS 'Auto Launch',
    '' AS Publisher,
    '' AS 'Uninstall String'
  FROM autoexec
)
WHERE
  'Program name' LIKE '%action1%'
  OR 'Program name' LIKE '%Ammyy%'
  OR 'Program name' LIKE '%AnyDesk%'
  OR 'Program name' LIKE '%Atera%'
  OR 'Program name' LIKE '%Dameware%'
  OR 'Program name' LIKE '%GoToAssist%'
  OR 'Program name' LIKE '%Intelliadmin%'
  OR 'Program name' LIKE '%Kaseya%'
  OR 'Program name' LIKE '%LogMeIn%'
  OR 'Program name' LIKE '%meshcentral%'
  OR 'Program name' LIKE '%NetSupport%'
  OR 'Program name' LIKE '%Radmin%'
  OR 'Program name' LIKE '%Remote%'
  OR 'Program name' LIKE '%RemotePC%'
  OR 'Program name' LIKE '%remotepc%'
  OR 'Program name' LIKE '%rustdesk%'
  OR 'Program name' LIKE '%ScreenConnect%'
  OR 'Program name' LIKE '%Simple%help%'
  OR 'Program name' LIKE '%Splashtop%'
  OR 'Program name' LIKE '%SupRemo%'
  OR 'Program name' LIKE '%tacticalrmm%'
  OR 'Program name' LIKE '%TeamViewer%'
  OR 'Program name' LIKE '%UltraViewer%'
  OR 'Program name' LIKE '%VNC%'
